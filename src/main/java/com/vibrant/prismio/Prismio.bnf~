simpleFile ::= item_*

private item_ ::= (property |
COMMENT | CRLF | IDENTIFIER | OPERATOR | STRING_LITERAL
| CHARACTER_LITERAL | BOOLEAN | MULTILINE_COMMENT | SINGLE_LINE_COMMENT | KEYWORD
| INTEGER | FLOAT
 )

